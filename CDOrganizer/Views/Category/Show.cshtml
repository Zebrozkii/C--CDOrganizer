<h3>@Model["category"].GetGenre()</h3>

<ol>
@foreach (var item in @Model["items"])
{
  <li><a href='/categories/@Model["category"].GetId()/items/@item.GetId()'>@item.GetTitle()</a></li>
}
</ol>

<p><a href='/categories/@Model["category"].GetId()/items/new'>Add a new item</a></p>
<p><a href='/categories'>Return to genre list</a></p>
<p><a href='/categories'>Return to Main Page</a></p>
